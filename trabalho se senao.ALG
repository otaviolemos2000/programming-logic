Algoritmo "SENAO&SE"

Var
   n1: real
Inicio
   escreva("DIGITE UM NUMERO: ")
   leia(n1)

   limpatela
   
   se n1>10 entao
      escreva ("É MAIOR QUE 10!")
      escreval("")
   
   FIMSE
   
   se n1<10 entao
      escreva ("É MENOR QUE 10!")
      escreval("")
   FIMSE
   
   se n1=10 entao
   escreva  ("É IGUAL A 10!")
   FIMSE
   
Fimalgoritmo